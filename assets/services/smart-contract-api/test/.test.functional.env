NODE_ENV=development
LOG_LEVEL=warn
KAFKAJS_LOG_LEVEL=warn

# Transaction orchestrator
DEFAULT_ETHSERVICE_TYPE=orchestrate

##############################################################################
######################## CONNECTION TO CODEFI MODULES #######################

# Assets-API
ASSETS_API_URL=http://assets-api-url/v2
ASSETS_API_SECRET=xxx

# Orchestrate
ORCHESTRATE_NEW_CONTRACT_TX=true
ORCHESTRATE_BROKER_HOST=kafka
ORCHESTRATE_BROKER_PORT=9092
ORCHESTRATE_URL=http://orchestrate-test-host

ORCHESTRATE_TOPIC_RECOVER=topic-tx-recover
ORCHESTRATE_TOPIC_DECODED=topic-tx-decoded
ORCHESTRATE_REGISTRY_TAG=latest
CODEFI_ENV_ID=codefi-local-env-001

# Network
DEFAULT_NETWORK_KEY=rinkeby

# Contracts directories
# Standard place to put contracts that will be deployed in Orchestrate registry
CONTRACTS_DIRECTORY=contracts
# Truffle build output of those standard contracts
CONTRACTS_TRUFFLE_BUILD_DIRECTORY=build/contracts
# We can also directly put build files in this folder
CONTRACTS_CUSTOM_BUILD_DIRECTORY=buildCustom

DOCKER_LAUNCH=true

# Authentication
AUTH0_URL=https://codefi.eu.auth0.com/

# Access token verification
AUTH_BYPASS_AUTHENTICATION_CHECK=false
AUTH_BYPASS_PERMISSION_CHECK=false
AUTH_ACCEPTED_AUDIENCE=https://api.codefi.network
AUTH_CUSTOM_NAMESPACE=https://api.codefi.network
AUTH_CUSTOM_ORCHESTRATE_NAMESPACE=https://api.orchestrate.network
AUTH_HMAC_SECRET=

# Access token creation
M2M_TOKEN_REDIS_ENABLE=false
M2M_TOKEN_REDIS_HOST=mock
M2M_TOKEN_REDIS_PASS=mock
M2M_TOKEN_CLIENT_ID=mock
M2M_TOKEN_CLIENT_SECRET=xxx
M2M_TOKEN_AUDIENCE=https://api.codefi.network

FAUCET_AMOUNT=300000000000000000 # To be converted into hex after Orchestrate breaking change https://consensys.slack.com/archives/GQF05NK51/p1637909977062100

FAUCET_MNEMONIC=click pond obey ensure monster ginger plug soup issue open uphold salt idea salon maze

INFURA_KEY=xxx-xxxxx-xxx

REAL_TOKEN=